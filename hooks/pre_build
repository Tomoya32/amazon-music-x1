#!/bin/bash
echo "=> Building Environmental Variables"
docker build --build-arg REACT_APP_AMAZON_MUSIC_CLIENT_ID=${REACT_APP_AMAZON_MUSIC_CLIENT_ID} --build-arg REACT_APP_SERIAL_NUMBER=${REACT_APP_SERIAL_NUMBER} --build-arg REACT_APP_MUSIC_ENDPOINT=https://music-api.amazon.com --build-arg REACT_APP_TEST_STRING=hello . -t $IMAGE_NAME